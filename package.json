{
  "name": "canvas-music",
  "version": "1.0.0",
  "description": "Визуальный проигрыватель мызыки",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vredsun/canvas-music.git"
  },
  "author": "vredsun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vredsun/canvas-music/issues"
  },
  "homepage": "https://github.com/vredsun/canvas-music#readme",
  "scripts": {
    "dev": "npm i && cross-env NODE_ENV=development PUBLIC_PATH='/' NODE_OPTIONS=--max-old-space-size=4096 webpack-dev-server",
    "build": "npm i && cross-env NODE_ENV=production PUBLIC_PATH='/canvas-music/' webpack",
    "test": "cross-env JEST_JUNIT_OUTPUT_DIR='./coverage' jest --coverage --colors",
    "test:watch": "cross-env JEST_JUNIT_OUTPUT_DIR='./coverage' jest --coverage --colors --colors --watch",
    "lint": "eslint --config ./.eslintrc.js src --ext .ts,.tsx",
    "lint:fix": "eslint --config ./.eslintrc.js src --ext .ts,.tsx --fix --quiet",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "polished": "^3.4.4",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.1",
    "thread-loader": "^2.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.0",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^4.0.4",
    "gh-pages": "^2.2.0",
    "handlebars": "^4.7.3",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.7",
    "react-hot-loader": "^4.12.19",
    "regenerator-runtime": "^0.13.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2",
    "typescript-styled-plugin": "^0.15.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --config ./.eslintrc.js --ext .ts,.tsx --fix --quiet"
    ]
  }
}
